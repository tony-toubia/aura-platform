{
  "crons": [
    {
      "path": "/api/cron/evaluate-rules",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/process-notifications",
      "schedule": "*/1 * * * *"
    }
  ],
  "functions": {
    "app/api/cron/evaluate-rules/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/process-notifications/route.ts": {
      "maxDuration": 30
    }
  }
}